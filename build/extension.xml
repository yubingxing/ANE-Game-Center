<extension xmlns="http://ns.adobe.com/air/extension/3.1">
	<id>com.icestar.GameKit</id>
	<versionNumber>0.1.0</versionNumber>
	<platforms>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libGameKitIosExtension.a</nativeLibrary>
				<initializer>GameKitExtensionInitializer</initializer>
				<finalizer>GameKitExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default">
			<applicationDeployment/>
		</platform>
	</platforms>
</extension>
